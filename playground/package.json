    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.3.8",
    "sass": "^1.35.1",
    "terser": "^5.14.2",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^5.0.7",
    "vue-tsc": "^1.8.25"
