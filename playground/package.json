  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@tmagic/core": "1.3.16",
    "@tmagic/design": "1.3.16",
    "@tmagic/editor": "1.3.16",
    "@tmagic/element-plus-adapter": "1.3.16",
    "@tmagic/form": "1.3.16",
    "@tmagic/tmagic-form-runtime": "1.0.2",
    "@tmagic/schema": "1.3.16",
    "@tmagic/stage": "1.3.16",
    "@tmagic/utils": "1.3.16",
    "element-plus": "^2.4.3",
    "monaco-editor": "^0.41.0",
    "serialize-javascript": "^6.0.0",
    "vue": "^3.3.8",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@types/node": "^18.19.0",
    "@types/serialize-javascript": "^5.0.1",
